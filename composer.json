    "description": "A PHP implementation of PKCS #8 encrypted private-key.",
    "homepage": "https://github.com/sop/pkcs8",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "pkcs8",
        "encrypt",
        "private-key"
    ],
    "authors": [
        {
            "name": "Joni Eskelinen",
            "email": "jonieske@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "sop/asn1": "^4.0.0",
        "sop/crypto-encoding": "^0.3.0",
        "sop/crypto-types": "^0.3.0",
        "sop/crypto-bridge": "^0.3.1",
        "sop/pkcs5": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Sop\\PKCS8\\": "lib/PKCS8/"
        }
    }
}